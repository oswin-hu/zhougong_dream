{
    "name": "oswin/zg_dream",
    "description": "基于laravel系统的周公解梦应用",
    "keywords": [
        "Zhou Gong",
        "Zhou Gong Dream",
        "Zhou Gong Dream Interpretation"
    ],
    "homepage": "https://github.com/oswin-hu/zhougong_dream",
    "license": "MIT",
    "authors": [
        {
            "name": "oswin.hu",
            "email": "oswin.hu@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/support": "^8 || ^9",
        "illuminate/database": "^8 || ^9"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZGDream\\Providers\\ZgDreamProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "ZGDream\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test/"
        }
    }
}
